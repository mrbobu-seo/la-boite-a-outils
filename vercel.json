{
  "rewrites": [
    {
      "source": "/api/proxy",
      "destination": "/api/proxy"
    },
    {
      "source": "/api/speedyindex-proxy/(.*)",
      "destination": "/api/speedyindex-proxy"
    },
    {
      "source": "/api/scraperapi-proxy/(.*)",
      "destination": "/api/scraperapi-proxy"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}